{
  "name": "heimrichhannot/contao-company-bundle",
  "type": "contao-bundle",
  "description": "This bundle offers functionality for the handling of companies.",
  "license": "LGPL-3.0-or-later",
  "require": {
    "php": "^7.1 || ^8.0",
    "contao/core-bundle": "^4.4",
    "heimrichhannot/contao-utils-bundle": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "HeimrichHannot\\CompanyBundle\\": "src/"
    },
    "classmap": [
      "src/"
    ],
    "exclude-from-classmap": [
      "src/Resources/contao/config/",
      "src/Resources/contao/dca/",
      "src/Resources/contao/languages/",
      "src/Resources/contao/templates/"
    ]
  },
  "config": {
    "preferred-install": "dist"
  },
  "extra": {
    "contao-manager-plugin": "HeimrichHannot\\CompanyBundle\\ContaoManager\\Plugin"
  }
}
